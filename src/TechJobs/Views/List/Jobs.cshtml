<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@foreach (Dictionary<string, string> job in ViewBag.Jobs)
{
    <table class="job-listing">
        @foreach (KeyValuePair<string, string> job_info in job)
        {
            <tr>
                <td>@job_info.Key: @job_info.Value</td>
            </tr>
        }
    </table>
    <hr />
}
}